[path]
data = ./data/ISRUC_S3/ISRUC_S3.npz
disM = ./data/ISRUC_S3/DistanceMatrix.npy
save = ./output/

[feature]
channels = 10
fold = 10
epoch_f = 80
batch_size_f = 64
optimizer_f = adam
learn_rate_f = 0.0002

[train]
channels = 10
fold = 10
context = 5
epoch = 80
batch_size = 32
optimizer = adam
learn_rate = 0.0002
lambda_GRL = 0.001

[model]
adj_matrix = fix
Globaldense = 64
GLalpha = 0.0001
cheb_filters = 64
time_filters = 64
time_conv_strides = 1
time_conv_kernel = 3
num_block = 1
cheb_k = 3
l1 = 0
l2 = 0
dropout = 0.5
